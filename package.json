{
    "name": "xo_front",
    "version": "0.1.0",
    "homepage": "https://github.com/octomen/xo_front",
    "dependencies": {
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "react-transition-group": "4.3.0"
    },
    "devDependencies": {
        "@types/react": "16.9.22",
        "@types/react-dom": "16.9.5",
        "@types/react-transition-group": "4.2.2",
        "gh-pages": "2.2.0",
        "npm-run-all": "4.1.5",
        "react-scripts": "3.4.0",
        "typescript": "3.7.5"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "deploy": "PUBLIC_URL=/xo_front run-s deploy:*",
        "deploy:build": "npm run build",
        "deploy:clean-cache": "rm -rf node_modules/gh-pages/.cache",
        "deploy:publish": "gh-pages -d build -r https://github.com/octomen/xo_front"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ]
    }
}
